[submodule ".ohmyzsh"]
	path = zshgit/.ohmyzsh
	url = https://github.com/ohmyzsh/ohmyzsh
[submodule "p10k"]
	path = zshgit/.zshplugins/themes/powerlevel10k
	url = https://github.com/romkatv/powerlevel10k
[submodule "zsh-syntax-highlighting"]
	path = zshgit/.zshplugins/plugins/zsh-syntax-highlighting
	url = https://github.com/zsh-users/zsh-syntax-highlighting
[submodule "zsh-autocomplete"]
	path = zshgit/.zshplugins/plugins/zsh-autocomplete
	url = https://github.com/marlonrichert/zsh-autocomplete
[submodule "nvim"]
	path = nvim
	url = https://github.com/yangavin/nvim/
	branch = main
